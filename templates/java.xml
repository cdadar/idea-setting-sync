<templateSet group="java">
  <template name="transactional" value="@Transactional(&#10;    rollbackFor = {CoreBaseException.class, Exception.class},&#10;    noRollbackFor = {WarningException.class})" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="jsonIgnoreProperties" value="@JsonIgnoreProperties(ignoreUnknown = true)" description="jackjson bean 的多余输入字段忽略" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="date" value="@DateTimeFormat(pattern = &quot;yyyy-MM-dd&quot;)&#10;" description="controller 参数的时间格式" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="time" value="@DateTimeFormat(pattern = &quot;yyyy-MM-dd HH:mm:ss&quot;)&#10;" description="controller 参数的时间格式" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="mapper" value="@Mapper(componentModel = &quot;spring&quot;)" description="mapstruct 类 spring 管理" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="log" value="private static final Logger LOG = LoggerFactory.getLogger($className$.class);&#10;" toReformat="false" toShortenFQNames="true">
    <variable name="className" expression="className()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>